class Expr
{
    public:
        Expr(int value);
        Expr(double value);
        Expr(string value);
        Expr(Gpl_op type, Expr *lhs, Expr *rhs) // for the operators
        
        int eval_int();
        double eval_double();
        string eval_string();
        
    private:
        string type;
        string kind;
}
