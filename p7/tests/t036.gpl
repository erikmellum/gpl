string str;
int i;
int size = 4;
rectangle rects[size];

on akey
{
    for (i = 0; i < size; i += 1)
    {
        rects[i].x = i * 100;
        rects[i].y = i * 100;
    }
}

on skey
{
    for (i = 0; i < size; i += 1)
    {
        str = "rects[" + i + "].x = " + rects[i].x;
        print(str);
        str = "rects[" + i + "].y = " + rects[i].y;
        print(str);
    }
}

